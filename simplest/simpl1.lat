int main () {
  Wrap w = new Wrap;
  w.setx(2);
  int x = w.getx();
  printInt(x);
  w.print();

  return 0;
}

class Wrap {
  int x;

  int getx() { return x; }

  void setx(int y) { x = y; }
  void print() {
    printString("class wrap with x:");
    printInt(x);
    printString("class wrap with getx:");
    printInt(self.getx());
  }
}

